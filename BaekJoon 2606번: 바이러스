#include <iostream>
#include <cstring>
#include <queue>

using namespace std;

int adjacent[101][101];
bool visited[101];
int n, m;
queue<int> q;
int cnt;
void dfs(int v)
{
	q.push(v); //1 
	visited[v]=1;
	
	while (!q.empty())
	{
		int a = q.front();
		q.pop();
		
		for (int i = 1; i <= n; i++)
		{
			if (adjacent[a][i] && !visited[i]) 			// 12 15 23 56
			{											// 12 15 23
				visited[i] = 1;
				q.push(i);
				cnt++;
			}
		}
	}
}

int main()
{
	cin >> n >> m;
	for (int i = 1; i <= m; i++)
	{
		int a, b;
		cin >> a >> b;
		adjacent[a][b] = 1;
		adjacent[b][a] = 1;
	}
	dfs(1);
	cout << cnt;
}

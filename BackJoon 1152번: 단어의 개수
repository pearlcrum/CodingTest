#include <iostream>
#include <vector>
#include <string>

using namespace std;

int main()
{
	string str;
	getline(cin, str);
	//띄어쓰기를 포함한 문자열을 저장하고자 할 때 Getline(cin, str)을 사용한다
	int count=1;
	
	for (int i = 0; i < str.size(); i ++)
	{
		if (i>=1 && str[i] == ' ')
		{
			count++;
		}
		if (((i + 1) == (str.size() - 1)) && (str[i + 1] == ' '))
		{
			count--;
		}
		if ((i == 0) && (str.size() == 1) && (str[i] == ' '))
		{
			count--;
		}
	}
	cout << count;

}
